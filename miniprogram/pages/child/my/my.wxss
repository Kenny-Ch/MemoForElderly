}
.today_content{
	width: 100%;
	color: rgba(0, 2, 2, 1);
	font-size: 32rpx;
	line-height: 150%;
	text-align: left;
	font-weight: bold;
	margin-bottom: 8rpx;
}
.time{
	width:100%;
	color: rgba(138, 87, 10, 1);
	font-size: 24rpx;
	line-height: 150%;
	text-align: left;
}
.isFinish{
	width: 84rpx;
	height: 42rpx;
	color: rgba(138, 87, 10, 1);
	font-size: 28rpx;
	line-height: 150%;
	text-align: left;
}
.pic{
	width: 40rpx;
	height: 40rpx;
	font-size: 40rpx;
	color: rgba(138, 87, 10, 1);
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
