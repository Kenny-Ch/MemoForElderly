/* miniprogram/pages/child/my/my.wxss */
page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.title{
  width: 120rpx;
	color: rgba(0, 2, 2, 1);
	font-size: 47rpx;
	text-align: left;
  font-weight: bold;
}
.arrow{
  width: 40rpx;
	height: 40rpx;
	font-size: 40rpx;
  color: rgba(153, 153, 153, 1);
  margin-left:8rpx;
  margin-top:8rpx;
}
.rePic{
  width: 70rpx;
	height: 70rpx;
	font-size: 70rpx;
	color: rgba(0, 2, 2, 1);
}
.top_card{
  width: 659rpx;
	height: 119rpx;
	color: rgba(80, 80, 80, 1);
	background-color: rgba(249, 186, 23, 1);
	border-radius: 16rpx;
	font-size: 28rpx;
	line-height: 150%;
  text-align: center;
  margin-top:20rpx;
}
.touxiang{
